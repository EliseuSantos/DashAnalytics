{{#acoes}}
  <div class="card card-block lista-fcas" data-idacao="{{id_acao}}">
  <h6 class="card-title"><span class="label label-primary">{{desc_projeto}}</span></h6>
    <div class="div-buttons">
      <button class="ativar-acao">
        <i class="fa fa-undo" aria-hidden="true"></i>
      </button>
    </div>
    <blockquote class="card-blockquote">
      <p>{{desc_acao}}</p>
    </blockquote>
  </div>
{{/acoes}}
