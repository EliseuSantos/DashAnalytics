<div class="card card-block card-progress-danger lanca-fcas" data-previsto="{{previsto}}" data-realizado="{{realizado}}" data-periodo="{{periodo}}" title="{{porcentagem}}%">
  <progress class="progress progress-success progress-bar-meta" value="100" max="100" style="width: {{porcentagem}}%">
    <div class="progress">
      <span class="progress-bar" style="{{realizado}}"></span>
    </div>
  </progress>
  <p class="text-md-center font-weight-bold card-text">{{nomePeriodo}}</p>
</div>